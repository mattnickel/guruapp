<div class= "header">
	<div class = "container">
		<div class="row">
			<span class=" three columns">
				<%= link_to home_path do%>
					<img src="/assets/lm_logo" class= "logo">
				<%end%>
			</span>

        <span class="offset-by-five four columns">

				  <% if current_user.present? %>
			      <div>Logged in as <%= current_user.email %> </div>
			      <%= link_to "Dashboard", dashboard_path, class: "button" %>
			      <%= link_to "Guru Logout", logout_path, class: "button" %>
			    <% else %>
			      <%= link_to "Get Started", new_user_registration_path, class: "button right" %>
			      <% link_to "Sign in", new_user_session_path, class: "right" %>
			    <% end %>
			  </span>
			</div>
		</div>
  </div>
</div>